{
  "name": "cdk",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "import": "cdk8s import",
    "synth": "cdk8s synth",
    "docker:ava": "docker build -t auser/ava-node ./docker/avalanchego",
    "docker:monitor": "docker build -t auser/ava-node ./docker/monitor",
    "clean": "tsc --build --clean",
    "deploy": "npm run compile && npm run synth && kubectl apply -f dist/cdk.k8s.yaml",
    "compile": "tsc -p .",
    "watch": "tsc -p . -w",
    "test": "jest",
    "build": "npm run compile && npm run test && npm run synth",
    "upgrade": "npm i cdk8s@latest cdk8s-cli@latest",
    "upgrade:next": "npm i cdk8s@next cdk8s-cli@next"
  },
  "dependencies": {
    "cdk8s": "^1.3.32",
    "cdk8s-plus-22": "^1.0.0-beta.77",
    "constructs": "^3.3.178",
    "dotenv": "^10.0.0",
    "kubernetes-client": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^14.18.4",
    "cdk8s-cli": "^1.0.75",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "selfsigned": "^2.0.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.5.4"
  }
}
