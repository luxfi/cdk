{
  "log-level": "verbo",
  "api-admin-enabled": true,
  "api-auth-required": true,
  "api-auth-password": "Please-change-me-to-1234!",
  "api-health-enabled": true,
  "api-info-enabled": true,
  "http-host": "",
  "network-id": "network-4200",
  "bootstrap-beacon-connection-timeout": "5m",
  "bootstrap-retry-enabled": true,
  "network-allow-private-ips": true,
  "db-dir": "/usr/share/.avalanchego/db",
  "index-enabled": false,
  "api-metrics-enabled": true,
  "genesis": "/etc/ava/genesis.json",
  "log-display-level": "Verbo",
  "log-display-highlight": "auto",
  "staking-port": 9651,
  "staking-enabled": true,
  "staking-tls-cert-file": "/etc/ava/staking.crt",
  "staking-tls-key-file": "/etc/ava/staking.key",
  "snow-sample-size": 1,
  "snow-quorum-size": 1,
  "signature-verification-enabled": false,
  "chain-config-dir": "/etc/ava/chains/",
  "network-require-validator-to-connect": false,
  "eth-apis": [
    "public-eth",
    "public-eth-filter",
    "net",
    "web3",
    "internal-public-eth",
    "internal-public-blockchain",
    "internal-public-transaction-pool"
  ]
}
